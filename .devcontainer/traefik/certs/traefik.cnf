[ req ]
default_bits            = 2048
encrypt_key             = no
default_md              = sha256
utf8                    = yes
string_mask             = utf8only
prompt                  = no
distinguished_name = req_distinguished_name
req_extensions     = req_ext
[ req_distinguished_name ]
countryName         = US
stateOrProvinceName = New York
localityName        = Rochester
organizationName    = Rochester Institute of Technology
organizationalUnitName = Identity and Access Management
commonName          = certificat.localtest.me
[ req_ext ]
subjectAltName = @alt_names
[alt_names]
DNS.1 = certificat.localtest.me
DNS.2 = idp.localtest.me